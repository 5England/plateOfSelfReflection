<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.devwan.plateofselfreflection">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PlateOfSelfReflection">
        <activity android:name=".UpdateNickNameActivity"></activity>
        <activity
            android:name=".PlateActivity"
            android:windowSoftInputMode="adjustResize" />
        <activity android:name=".UploadFeedbackActivity" />
        <activity android:name=".UpdatePlateActivity" />
        <activity android:name=".UploadPlateActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>